<?xml version="1.0" encoding="UTF-8"?>
<function>
   <name>create.one</name>
   <title>Get one user</title>
   <description>This function get all user data from database table</description>
   <visibility>private</visibility>
   <get>
      <data key="user_username" type="string" rules="required|callback_username" desc="Unique username of user" necessary='optional'>
  		 	username
      </data>
      <data key="user_email" type="string" rules="required|validateEmail" desc="Unique email of user" necessary='required'>
      		example@example.com
      </data>
   </get>
   <version>1.0</version>
   <author>Hakan Garip</author>
   <publish_date>2000-10-01</publish_date>
</function>